APP_ENV=production
API_PREFIX=/api/v1
DATABASE_URL=postgresql://docuhub:${POSTGRES_PASSWORD}@postgres-prod:5432/docuhub_prod
DEBUG=false
JWT_SECRET=${JWT_SECRET}
JWT_TTL_SECONDS=3600
RATE_LIMIT_REQUESTS=600
RATE_LIMIT_WINDOW_S=60
CORS_ALLOWED_ORIGINS=https://app.docuhub.example
HSTS_ENABLED=true
URL_DOWNLOAD_TIMEOUT_S=5
EXTRACT_TIMEOUT_S=3
MAX_DOWNLOAD_CHARS=20000
MAX_UPLOAD_CHARS=200000
CONCURRENCY_LIMIT=80
