APP_ENV=staging
API_PREFIX=/api/v1
DATABASE_URL=postgresql://docuhub:docuhub@postgres-staging:5432/docuhub_staging
DEBUG=false
JWT_SECRET=staging-change-me
JWT_TTL_SECONDS=3600
RATE_LIMIT_REQUESTS=240
RATE_LIMIT_WINDOW_S=60
CORS_ALLOWED_ORIGINS=https://staging.docuhub.example
HSTS_ENABLED=true
URL_DOWNLOAD_TIMEOUT_S=5
EXTRACT_TIMEOUT_S=3
MAX_DOWNLOAD_CHARS=20000
MAX_UPLOAD_CHARS=200000
CONCURRENCY_LIMIT=40
